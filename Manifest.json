{
   "name": "EasySec",
   "description": "An Easy Browser Security Suit that adds simple checks to prevent social engineering attacks",
   "icons": {
      "128": "images/icon-128x128.png",
      "48": "images/icon-48x48.png"
   },
   "version": "1.0",
   "manifest_version": 2,
   "browser_action": {
      "default_icon": {
         "128": "images/icon-128x128.png",
        "48": "images/icon-48x48.png",
         "19": "images/icon-48x48.png"
      },
      "default_title": "EasySec"
   },
   "content_scripts": [ {
      "all_frames": true,

      "js": [ "scripts/jquery-1.10.2.min.js", "scripts/TypoSquatting/levenshtein.js","scripts/TypoSquatting/typosquatting.js", "scripts/LinkExpansion/LinkExpansion.js", "scripts/PasswordEncrypt/passwordEncrypt.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   }
   ],
   "minimum_chrome_version": "26.0.0.0",
   "permissions": [ "http://*/*", "https://*/*", "cookies", "idle", "notifications", "tabs","storage" ],
   "web_accessible_resources": [ "*" ]
}