{
   "background": {
      "persistent": false,
      "scripts": [ "scripts/bg.js" ]
   },
   "browser_action": {
      "default_icon": {
         "128": "images/icon-128x128.png",
        "48": "images/icon-48x48.png",
         "19": "images/icon-48x48.png"
      },
      "default_title": "EasySec"
   },
   "content_scripts": [ {
      "all_frames": true,

      "js": [ "scripts/jquery-1.10.2.min.js", "scripts/PasswordEncrypt/aes-enc.js", "scripts/PasswordEncrypt/aes-dec.js", "scripts/PasswordEncrypt/aes-test.js" ,"scripts/TypoSquatting/levenshtein.js","scripts/TypoSquatting/typosquatting.js", "scripts/LinkExpansion/LinkExpansion.js", "scripts/PasswordEncrypt/passwordEncrypt.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   }
   ],
   "description": "An Easy Browser Security Suit",
   "icons": {
      "128": "images/icon-128x128.png",
      "48": "images/icon-48x48.png"
   },
   "manifest_version": 2,
   "minimum_chrome_version": "26.0.0.0",
   "name": "EasySec",
   //"options_page": "settingsdialog.html",
   "version": "0.0.0.1",
   "permissions": [ "http://*/*", "https://*/*", "background", "cookies", "idle", "notifications", "tabs","storage" ],
   "web_accessible_resources": [ "*" ]
}
